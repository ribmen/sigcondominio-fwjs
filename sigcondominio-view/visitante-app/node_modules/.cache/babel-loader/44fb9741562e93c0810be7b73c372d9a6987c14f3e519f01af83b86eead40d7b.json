{"ast":null,"code":"import axios from 'axios';\nimport EventBus from '../event-bus'; // Importa o Event Bus\n\nexport default {\n  name: 'ListarVisitantesComponent',\n  data() {\n    return {\n      visitantes: []\n    };\n  },\n  methods: {\n    async listarVisitantes() {\n      try {\n        const response = await axios.get('http://localhost:8081/visitante');\n        this.visitantes = response.data;\n      } catch (error) {\n        console.error('Erro ao listar visitantes:', error);\n        alert('Falha ao listar visitantes.');\n      }\n    }\n  },\n  created() {\n    EventBus.config.globalProperties.$on('visitanteAdicionado', this.listarVisitantes); // Ouve o evento do Event Bus\n  },\n  beforeUnmount() {\n    EventBus.config.globalProperties.$off('visitanteAdicionado', this.listarVisitantes); // Remove o listener do evento ao desmontar o componente\n  }\n};","map":{"version":3,"names":["axios","EventBus","name","data","visitantes","methods","listarVisitantes","response","get","error","console","alert","created","config","globalProperties","$on","beforeUnmount","$off"],"sources":["C:\\Users\\vgmen\\OneDrive\\Documentos\\Faculdade\\5 SEMESTRE\\web-1\\sigcondominio\\sigcondominio-view\\visitante-app\\src\\components\\ListarVisitantesComponent.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>Lista de Visitantes</h2>\r\n    <button @click=\"listarVisitantes\">Listar Visitantes</button>\r\n    <ul>\r\n      <li v-for=\"visitante in visitantes\" :key=\"visitante.id\">\r\n        {{ visitante.nome }} - {{ visitante.cpf }}\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport EventBus from '../event-bus'; // Importa o Event Bus\r\n\r\nexport default {\r\n  name: 'ListarVisitantesComponent',\r\n  data() {\r\n    return {\r\n      visitantes: []\r\n    };\r\n  },\r\n  methods: {\r\n    async listarVisitantes() {\r\n      try {\r\n        const response = await axios.get('http://localhost:8081/visitante');\r\n        this.visitantes = response.data;\r\n      } catch (error) {\r\n        console.error('Erro ao listar visitantes:', error);\r\n        alert('Falha ao listar visitantes.');\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    EventBus.config.globalProperties.$on('visitanteAdicionado', this.listarVisitantes); // Ouve o evento do Event Bus\r\n  },\r\n  beforeUnmount() {\r\n    EventBus.config.globalProperties.$off('visitanteAdicionado', this.listarVisitantes); // Remove o listener do evento ao desmontar o componente\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n/* Adicione seu CSS aqui, se necess√°rio */\r\n</style>\r\n"],"mappings":"AAaA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,QAAO,MAAO,cAAc,EAAE;;AAErC,eAAe;EACbC,IAAI,EAAE,2BAA2B;EACjCC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,gBAAgBA,CAAA,EAAG;MACvB,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMP,KAAK,CAACQ,GAAG,CAAC,iCAAiC,CAAC;QACnE,IAAI,CAACJ,UAAS,GAAIG,QAAQ,CAACJ,IAAI;MACjC,EAAE,OAAOM,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,4BAA4B,EAAEA,KAAK,CAAC;QAClDE,KAAK,CAAC,6BAA6B,CAAC;MACtC;IACF;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRX,QAAQ,CAACY,MAAM,CAACC,gBAAgB,CAACC,GAAG,CAAC,qBAAqB,EAAE,IAAI,CAACT,gBAAgB,CAAC,EAAE;EACtF,CAAC;EACDU,aAAaA,CAAA,EAAG;IACdf,QAAQ,CAACY,MAAM,CAACC,gBAAgB,CAACG,IAAI,CAAC,qBAAqB,EAAE,IAAI,CAACX,gBAAgB,CAAC,EAAE;EACvF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}